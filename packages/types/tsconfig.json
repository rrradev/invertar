{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "composite": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo/types.tsbuildinfo",
    "outDir": "dist",
    "rootDir": "."
  },
  "include": ["**/*"],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
